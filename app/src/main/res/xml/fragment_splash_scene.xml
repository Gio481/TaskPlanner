<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="5000">
        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/logoBackground"
                motion:percentY="0.5" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/logoFrame"
                motion:percentX="0.9"
                motion:percentY="0.5" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/logoBackground"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:alpha="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/logoFrame"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:alpha="0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoTopCheckedButton"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:alpha="-1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoUncheckedButton"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:alpha="-1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="parent" />
        <Constraint
            android:id="@+id/logoTopText"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:alpha="-1"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoBottomUncheckedButton"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:layout_marginTop="@dimen/dimen_70_dp"
            android:alpha="-1"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/logoMiddleText"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:layout_marginTop="@dimen/dimen_30_dp"
            android:alpha="-1"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoBottomText"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:layout_marginTop="@dimen/dimen_70_dp"
            android:alpha="-1"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/logoBackground"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoFrame"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoTopCheckedButton"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoUncheckedButton"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoTopText"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoBottomUncheckedButton"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:layout_marginTop="@dimen/dimen_70_dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoMiddleText"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:layout_marginTop="@dimen/dimen_30_dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/logoBottomText"
            android:layout_width="@dimen/dimen_150_dp"
            android:layout_height="@dimen/dimen_150_dp"
            android:layout_marginTop="@dimen/dimen_70_dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

    </ConstraintSet>
</MotionScene>